{
  "name": "vue",
  "version": "1.0.0",
  "description": "又一个mint薄荷糖脚手架 项目",
  "author": {
    "name": "花夏",
    "email": "liubiao0810@live.cn"
  },
  "keywords": [
    "generator-mint",
    "mint",
    "vue"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "mock": "cross-env VUE_APP_MOCK=1 npm run serve",
    "remote": "cross-env VUE_APP_MOCK=0 npm run serve",
    "mock:https": "cross-env VUE_APP_MOCK=1 npm run serve:https",
    "remote:https": "cross-env VUE_APP_MOCK=0 npm run serve:https",
    "serve:https": "cross-env NODE_ENV=development vue-cli-service serve --https",
    "serve": "cross-env NODE_ENV=development vue-cli-service serve",
    "dev:test": "cross-env NODE_ENV=testing vue-cli-service serve --mode release",
    "dev": "cross-env NODE_ENV=development vue-cli-service serve --mode development",
    "build:test": "vue-cli-service build --mode release",
    "build:dev": "vue-cli-service build --mode development",
    "build:pro": "vue-cli-service build --mode production",
    "lint": "vue-cli-service --no-fix lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "gitHooks": {
    "pre-commit": "npm run lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "dayjs": "^1.8.19",
    "less": "^2.7.3",
    "less-loader": "^5.0.0",
    "style-loader": "^1.1.2",
    "style-resources-loader": "^1.3.3",
    "view-design": "^4.2.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2",
    "vuex-persistedstate": "^2.7.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-component": "^1.1.1",
    "cross-env": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "mockjs": "^1.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "vue-template-compiler": "^2.6.10",
    "yorkie": "^2.0.0"
  }
}
